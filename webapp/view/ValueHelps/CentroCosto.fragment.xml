<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>centroCosto}"
		items="{path: 'localModel>/centroCosto', templateShareable: false }"
		search="_handleValueHelpSearchCentroCosto"
		liveChange="_handleValueHelpSearchCentroCosto"
		confirm="_handleValueHelpConfirmCentroCosto"
		cancel="_handleValueHelpCloseComisionista"
		growingThreshold="1000"
		showClearButton="true"
		multiSelect="true">
		<StandardListItem
			selected="{localModel>Selected}"
			iconDensityAware="false"
			iconInset="false"
			description="{localModel>OptionName}"
			title="{localModel>OptionName}" />
	</SelectDialog>
</core:FragmentDefinition>